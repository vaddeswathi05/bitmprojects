    "s=input()\n",
    "i=0\n",
    "n=len(s)-1\n",
    "while(i<n):\n",
    "    if(s[i]!=s[n]):\n",
    "        print(\"not a palindrome\")\n",
    "        break\n",
    "    i=i+1\n",
    "    n=n-1\n",
    "else:\n",
    "        print(\"is a palindrome\")"
