    "    def dad(self):\n",
    "        print(\"we are in dad's class\")\n",
    "class child(father):\n",
    "    def son(self):\n",
    "        print(\"we are in koduku class\")\n",
    "o=child()\n",
    "o.grand()\n",
    "o.dad()\n",
    "o.son()"
